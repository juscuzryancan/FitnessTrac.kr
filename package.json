{
  "name": "fitnesstrackr",
  "version": "1.0.0",
  "description": "an API for our new fitness empire, FitnessTrac.kr, using node, express, and postgresql",
  "main": "index.js",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "seed:dev": "nodemon ./db/seed.js",
    "seed": "node ./db/seed.js",
    "server:dev": "nodemon index.js",
    "start:dev": "nodemon index.js",
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand",
    "test:watch:db": "jest --watchAll db",
    "test:watch:api": "jest --watchAll api",
    "client:dev": "react-scripts start",
    "tailwind:watch": "npx tailwindcss -i ./src/index.css -o ./public/output.css --watch",
    "heroku:build": "react-scripts --openssl-legacy-provider build",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpuzon01/FitnessTrac.kr.git"
  },
  "keywords": [
    "node",
    "express",
    "postgresql",
    "javascript",
    "react"
  ],
  "author": "Ryan Riley Puzon",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^2.0.4",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "clsx": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.44.3",
    "react-icons": "^4.10.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.2.1"
  },
  "proxy": "http://localhost:3000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
